{
  "name": "gwitch",
  "version": "0.0.0",
  "description": "electron git client",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . $npm_package_name --prune --asar --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=.git"
  },
  "keywords": [
    "git",
    "electron"
  ],
  "author": "Jon Spencer",
  "license": "ISC",
  "devDependencies": {
    "electron-prebuilt": "^0.26.1",
    "electron-rebuild": "^0.1.4"
  },
  "dependencies": {
    "backbone": "^1.2.0",
    "fs-jetpack": "^0.6.4",
    "handlebars": "^3.0.3",
    "jquery": "^2.1.4",
    "moment": "^2.10.3",
    "nodegit": "git+https://github.com/nodegit/nodegit.git",
    "underscore": "^1.8.3"
  },
  "engines": {
    "electron": "0.26.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhs67/gwitch.git"
  },
  "bugs": {
