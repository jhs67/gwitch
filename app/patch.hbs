<div class="patch-header">{{file}}</div>
{{#each msgs}}
<div class="diff-msg">{{msg}}</div>
{{/each}}
{{#if hunks}}
<table>
{{#each hunks}}
	<tr class="hunk">
		<td>...</td>
		<td>...</td>
		<td></td>
		<td>{{header}}</td>
	</tr>
	{{#each lines}}
	<tr class="diff-line {{classes}}">
		<td>{{oldLineno}}</td>
		<td>{{newLineno}}</td>
		<td>{{origin}}</td>
		<td>{{content}}</td>
	</tr>
	{{/each}}
{{/each}}
</table>
{{else if large}}
<div class="large-diff">{{lines}} lines: click to open</div>
{{else if loading}}
<div class="loading">loading...</div>
{{/if}}
